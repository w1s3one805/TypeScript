>(cond ? undefined : 32) : 32
>                        : ^^
>cond ? undefined : 32 : 32
>                      : ^^
>cond : any
>     : ^^^
>undefined : undefined
>          : ^^^^^^^^^
>32 : 32
>   : ^^
>"possibly reached" : "possibly reached"
>                   : ^^^^^^^^^^^^^^^^^^

// Not OK: Both operands nullish
const test2 = (cond ? undefined : null) ?? "always reached";
